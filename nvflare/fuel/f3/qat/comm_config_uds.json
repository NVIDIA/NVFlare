{
  "internal": {
    "scheme": "uds",
    "resources": {
      "host": "localhost"
    }
  },
  "external": {
    "scheme": "grpc",
    "allow_adhoc": false,
    "resources": {
      "host": "localhost",
      "secure": true,
      "ports": ["8003-9000"]
    }
  },
  "backbone": {
    "connect_generation": 2
  },
  "grpc": {
    "max_workers": 20,
    "options": [
      [
        "grpc.max_send_message_length", 1073741824
      ],
      [
        "grpc.max_receive_message_length", 1073741824
      ]
    ]
  }
}