{
  "format_version": 2,
  "min_clients": 6,
  "num_rounds": 150,
  "server": {
    "heart_beat_timeout": 600000
  },
  "task_data_filters": [],
  "task_result_filters": [],
  "components": [
    {
      "id": "persistor",
      "path": "pt.persistors.pt_file_fedsm_model_persistor.PTFileFedSMModelPersistor",
      "args": {
        "client_ids": [
          "client_I2CVB",
          "client_MSD",
          "client_NCI_ISBI_3T",
          "client_NCI_ISBI_Dx",
          "client_Promise12",
          "client_PROSTATEx"
        ],
        "model": {
          "path": "monai.networks.nets.unet.UNet",
          "args": {
            "dimensions": 2,
            "in_channels": 1,
            "out_channels": 1,
            "channels": [
              16,
              32,
              64,
              128,
              256
            ],
            "strides": [
              2,
              2,
              2,
              2
            ],
            "num_res_units": 2
          }
        },
        "model_selector": {
          "path": "pt.networks.vgg.vgg11",
          "args": {
            "num_classes": 6
          }
        }
      }
    },
    {
      "id": "shareable_generator",
      "path": "pt.shareablegenerators.full_model_shareable_fedsm_generator.FullModelShareableFedSMGenerator",
      "args": {}
    },
    {
      "id": "aggregator",
      "path": "pt.aggregators.accumulate_model_aggregator_personalized_softpull.AccumulateWeightedAggregatorPersonalizedSoftPull",
      "args": {}
    },
    {
      "id": "model_selector",
      "path": "pt.widgets.intime_model_fedsm_selector.IntimeModelFedSMSelector",
      "args": {
        "weigh_by_local_iter": true
      }
    }
  ],
  "workflows": [
    {
      "id": "scatter_and_gather",
      "path": "pt.workflows.scatter_and_gather_fedsm.ScatterAndGatherFedSM",
      "args": {
        "client_id_label_mapping": {
          "client_I2CVB": 0,
          "client_MSD": 1,
          "client_NCI_ISBI_3T": 2,
          "client_NCI_ISBI_Dx": 3,
          "client_Promise12": 4,
          "client_PROSTATEx": 5
        },
        "min_clients": "{min_clients}",
        "num_rounds": "{num_rounds}",
        "start_round": 0,
        "wait_time_after_min_received": 10,
        "aggregator_id": "aggregator",
        "persistor_id": "persistor",
        "shareable_generator_id": "shareable_generator",
        "train_task_name": "train",
        "train_timeout": 0
      }
    }
  ]
}
