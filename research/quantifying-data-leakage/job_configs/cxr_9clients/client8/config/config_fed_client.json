{
    "format_version": 2,
    "DATA_ROOT": "./data/COVID-19_Radiography_Dataset",
    "DATASET_JSON": "./data/data_200val+rest_client8.json",
    "BATCH_SIZE": 8,
    "executors": [
        {
            "tasks": [
                "train",
                "submit_model",
                "validate"
            ],
            "executor": {
                "id": "Executor",
                "path": "nvflare.app_common.executors.learner_executor.LearnerExecutor",
                "args": {
                    "learner_id": "learner"
                }
            }
        }
    ],
    "task_result_filters": [
        {
            "tasks": [
                "train"
            ],
            "filters": [
                {
                    "path": "nvflare_gradinv.GaussianPrivacy",
                    "args": {
                        "sigma0": 0,
                        "max_percentile": 95
                    }
                }
            ]
        }
    ],
    "task_data_filters": [],
    "components": [
        {
            "id": "learner",
            "path": "nvflare_gradinv.CXRLearner",
            "args": {
                "data_root": "{DATA_ROOT}",
                "dataset_json": "{DATASET_JSON}",
                "aggregation_epochs": 1,
                "lr": 0.01,
                "batch_size": "{BATCH_SIZE}",
                "num_class": 2
            }
        }
    ]
}