{
  "format_version": 2,
  "executors": [
    {
      "tasks": [
        "train",
        "validate",
        "submit_model"
      ],
      "executor": {
        "path": "fedhca2_learner.FedHCA2Learner",
        "args": {
          "train_config_filename": "config_train.json",
          "aggregation_epochs": 1,
          "analytic_sender_id": "analytic_sender"
        }
      }
    }
  ],
  "task_result_filters": [],
  "task_data_filters": [],
  "components": [
    {
      "id": "analytic_sender",
      "name": "AnalyticsSender",
      "args": {}
    },
    {
      "id": "event_to_fed",
      "name": "ConvertToFedEvent",
      "args": {
        "events_to_convert": [
          "analytix_log_stats"
        ],
        "fed_event_prefix": "fed."
      }
    }
  ]
}