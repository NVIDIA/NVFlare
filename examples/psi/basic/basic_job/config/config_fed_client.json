    {
      "tasks": [
        "PSI"
      ],
      "executor": {
        "id": "Executor",
        "path": "nvflare.app_common.executors.psi.psi_executor.PSIExecutor",
        "args": {
          "local_psi_id": "sample_psi"
        }
      }
    }
  ],
  "components": [
