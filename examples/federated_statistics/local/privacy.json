{
  "scopes": [
    {
      "name": "data-exploratory-policy",
      "task_result_filters": [
        {
          "name": "StatisticsPrivacyFilter",
          "args": {
            "result_filter_ids": [
              "min_count_checker",
              "min_max_noise_generator",
              "hist_bins_checker"
            ]
          }
        }
      ],
      "default_scope": "public",
      "task_data_filters": [
        {
        }
      ]
    }
  ],

  "default_scope": "data-exploratory-policy",
  "components": [
    {
      "id": "min_max_noise_generator",
      "path": "nvflare.app_common.filters.statistics.min_max_noise_generator.AddNoiseToMinMax",
      "args": {
        "min_noise_level": 0.1,
        "max_noise_level": 0.3
      }
    },
    {
      "id": "hist_bins_check",
      "path": "nvflare.app_common.filters.statistics.histogram_bins_checker.HistogramBinsChecker",
      "args": {
        "max_bins_percent": 10
      }
    },
    {
      "id": "min_count_check",
      "path": "nvflare.app_common.filters.statistics.min_count_checker.MinCountChecker",
      "args": {
        "min_count": 10
      }
    }
  ]
}