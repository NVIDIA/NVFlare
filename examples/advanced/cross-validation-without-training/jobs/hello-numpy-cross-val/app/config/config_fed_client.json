{
  "format_version": 2,
  "MODEL_DIR": "{$CLIENT_MODEL_DIR}",
  "executors": [
    {
      "tasks": [
        "train",
        "submit_model"
      ],
      "executor": {
        "path": "nvflare.app_common.np.np_trainer.NPTrainer",
        "args": {
          "model_dir": "{MODEL_DIR}"
        }
      }
    },
    {
      "tasks": [
        "validate"
      ],
      "executor": {
        "path": "nvflare.app_common.np.np_validator.NPValidator"
      }
    }
  ],
  "task_result_filters": [],
  "task_data_filters": [],
  "components": []
}